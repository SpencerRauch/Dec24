@model LogUser
<form asp-action="LoginUser" asp-controller="User" method="POST" class="col-6">
    <h2>Log In</h2>
    <div class="form-group">
        <label asp-for="LogEmail"></label>
        <input asp-for="LogEmail" class="form-control" value="bob@bob.com"/>
        <span asp-validation-for="LogEmail" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="LogPassword"></label>
        <input asp-for="LogPassword" class="form-control" value="password"/>
        <span asp-validation-for="LogPassword" class="text-danger"></span>
    </div>
    <button class="btn btn-primary mt-3">Log In</button>
</form>
